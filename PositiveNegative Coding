package exercise;

public class PositiveNegative {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int x,y;
        x = 10;
        y = -10;
        
        if (x > 0 && y > 0)
        {
        	System.out.println("Both num is positive");
        }
        else if (x > 0 || y > 0)
        {
        	System.out.println("At least one num is positive");
        }
        else {
        
        }
	}

}
